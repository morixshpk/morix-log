<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Morix</RootNamespace>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <AssemblyName>Morix.Log</AssemblyName>
    <Authors>Morix shpk</Authors>
    <Company>Morix shpk</Company>
    <Description>c# library for logging to file</Description>
    <PackageProjectUrl>https://github.com/morixshpk/morix-log</PackageProjectUrl>
    <RepositoryUrl>https://github.com/morixshpk/morix-log</RepositoryUrl>
    <PackageLicenseFile>License.txt</PackageLicenseFile>
    <AssemblyVersion>1.0.1</AssemblyVersion>
    <FileVersion>1.0.1</FileVersion>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Diagnostics.EventLog" Version="6.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

   <ItemGroup>
    <None Include="..\License.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
